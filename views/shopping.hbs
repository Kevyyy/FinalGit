<div id="padding"></div>
{{#each products }}
    <div id="container">
        {{#each this }}
                <div class="thumbnail">
                    <img src="{{ this.imagePath }}" alt="..." class="img-responsive">
                    <div class="caption">
                        <h3>{{ this.title }}</h3>
                        <h1 class="description">{{ this.description }}</h1>
                        <div class="clearfix">
                            <div class="price pull-left">${{ this.price }}</div>
                            <a href='/addcart/{{this._id}}' >adsfads</a>
                            <button onclick='/addcart/'type="submit" class="btn btn-succcess btn-lg">buy ff now</button>
                        </div>
                    </div>
                </div>
        {{/each}}
    </div>
{{/each}}
</div>
