<div id="container">
    {{#if Items}}
    {{#each Items}}
    <div id="orders">
        <div id="orders-header">
            <div class="headers-details">
                <a>((Item placed Date))</a>
            </div>
            <div class="headers-details">
                <a>{{ this.status }}</a>
            </div>
            <div class="headers-details">
                <a>((Item arrival date))</a>
            </div>
        </div>
        <p id="item">{{this.stock_type}}</p>
        <p id="price">{{his.stock_price}}</p>
    </div>
</div>
{{/each}}
</div>
{{else}}
<div class="row">
    <div class="col-sm-6 col-md-6 col-md-offset-3 col-sm-offset-3">
        <h2>No Items in Cart</h2>
    </div>
</div>
{{/if}}